{
  "name": "bsb-api",
  "version": "0.1.4",
  "description": "Core API for the BSB platform, providing backend services and data access via REST.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test-watch": "vitest",
    "test-coverage": "vitest run --coverage",
    "prepare": "test \"$SKIP_HUSKY\" = \"true\" || husky install",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prisma:seed:instruments": "js-node prisma/instrumentsSeed.js",
    "prisma:seed:genres": "node prisma/genresSeed.js",
    "prisma:seed:socialMedia": "node prisma/socialMediaSeed.js",
    "prisma:seed": "node prisma/seed.js",
    "build": "echo 'no build step yet'"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "postinstall": "npm prune --omit=dev",
  "author": "MADM Tech Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.775.0",
    "@prisma/client": "^6.7.0",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "discord.js": "^14.18.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mailersend": "^2.4.0",
    "multer": "^1.4.5-lts.2",
    "multer-s3": "^3.0.1"
  },
  "devDependencies": {
    "@types/mime-types": "^2.1.4",
    "@vitest/coverage-v8": "^3.1.3",
    "husky": "^8.0.0",
    "mysql2": "^3.2.4",
    "nodemon": "^3.1.9",
    "prisma": "^6.7.0",
    "supertest": "^7.0.0",
    "vitest": "^3.1.3"
  }
}
